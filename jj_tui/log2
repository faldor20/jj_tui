       53:   36     : let (<-$) f v=Lwd.map ~f (Lwd.get v);; 
       54:   37     : let vShowStatus= Lwd.var "";;
       55:   38     : let inputs ui=
       56:   39     :   Ui.event_filter (fun event->
       57:   40     :     match event with
       58:   41     :     |`Key (`ASCII 'l',_)-> 
       59:   42     :         let res=cmd "jj"in
       60:   43     :         vcount$=res;
       61:   44     : 
       62:   45     :         `Handled
