        4:    %%NAME%% %%VERSION%%
        5:   ---------------------------------------------------------------------------*)
        6: 
        7: type ret = [ `Await | `Boundary | `End | `Uchar of Uchar.t ]
        8: 
        9: type t
       10: val create : unit -> t
       11: val add : t -> [ `Await | `End | `Uchar of Uchar.t ] -> ret
       12: 
