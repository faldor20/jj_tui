       15:   simpleterm_lwt ~s:1
       16:     ~f:(fun s -> function
       17:       | `Key (`ASCII 'q', _) -> None
       18:       | `Key (`Arrow a, _) ->
       19:         ( match a with
       20:           | `Up | `Left -> Some (max 1 (s - 1))
       21:           | `Down | `Right -> Some (min 10 (s + 1)) )
       22:       | _ -> Some s)
       23:     ~imgf:I.(fun _ s ->
       24:       string A.empty (string_of_int s) <->
       25:       pad ~l:2 ~t:1 (Images.sierp A.magenta s)
