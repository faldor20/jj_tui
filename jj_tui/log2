       90:       "s1", "a"
       91:     ; "s2", "abcdefghij"
       92:     ; "s3", String.repeat 10 "abcdefghij"
       93:     ; "s4", String.repeat 100 "abcdefghij"
       94:     ; "u1", "â˜­"
