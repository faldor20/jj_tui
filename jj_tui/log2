      887:         7:   let _exit_code = Unix.system (mk_home_cmd cmd) in
      888:         8:   ()
      889:         9: 
      890:        10: let proc cmd =
      891:        11:   Printf.eprintf "ğŸš  %s\n%!" cmd;
      892:        12:   proc_silent cmd
