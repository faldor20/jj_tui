     2548:        82:   let attr = A.(fg lightmagenta) in
     2549:        83:   let img = I.(
     2550:        84:     centered attr text
     2551:        85:     |> vpad_sp attr 1 1 |> hpad_sp attr 2 2
     2552:        86:     |> Images.outline attr
     2553:        87:     |> pad ~t:1 ~b:1 ~l:2 ~r:2
