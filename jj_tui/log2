      894:        14: let collect_chan (channel : in_channel) : string =
      895:        15:   let rec loop acc =
      896:        16:     match input_line channel with
      897:        17:     | exception End_of_file -> acc
      898:        18:     | line -> loop (acc ^ line ^ "\n")
      899:        19:   in
      900:        20:   loop ""
