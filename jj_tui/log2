     2916:         7: (* [Uucp.Break.tty_width_hint]. *)
     2917:         8: 
     2918:         9: val grapheme_cluster_boundary : Uchar.t -> int
     2919:        10: (* [Uucp.Break.Low.grapheme_cluster]. *)
