     1890:        19:       range 0 (w - a) |> map (fun b ->
     1891:        20:         i |> I.hcrop a b |> hpadwith A.(fg lightblack) '.' a b
     1892:        21:       ) |> I.vcat |> I.hpad 1 1
     1893:        22:     ) |> I.hcat |> I.vpad 1 1
     1894:        23:   )
