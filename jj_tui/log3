     [38;5;2m3592[39m: [38;5;2m      207: [39m
     [38;5;2m3593[39m: [38;5;2m      208:   let i x = x land 0xff[39m
     [38;5;2m3594[39m: [38;5;2m      209:   and r x = x lsr 16 land 0xff[39m
     [38;5;2m3595[39m: [38;5;2m      210:   and g x = x lsr 8 land 0xff[39m
