     [38;5;2m 101[39m: [38;5;2m[39m
     [38;5;2m 102[39m: [38;5;2m  let rec cantor = function[39m
     [38;5;2m 103[39m: [38;5;2m    | 0 -> square A.lightblue[39m
     [38;5;2m 104[39m: [38;5;2m    | n ->[39m
