     [38;5;2m5411[39m: [38;5;2m      113:     let rec t = lazy {[39m
     [38;5;2m5412[39m: [38;5;2m      114:         trm    = Tmachine.create ~mouse ~bpaste (cap_for_fd fd)[39m
