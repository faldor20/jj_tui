     [38;5;2m3575[39m: [38;5;2m      190: [39m
     [38;5;2m3576[39m: [38;5;2m      191:   let tag c = (c land 0x03000000) lsr 24[39m
     [38;5;2m3577[39m: [38;5;2m      192: [39m
     [38;5;2m3578[39m: [38;5;2m      193:   let rgb ~r ~g ~b =[39m
